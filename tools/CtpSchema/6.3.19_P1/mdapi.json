{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "行情接口",
  "type": "object",
  "properties": {
    "CThostFtdcMdSpi": {
      "title": "行情回调类",
      "properties": {
        "OnFrontConnected": {
          "title": "当客户端与交易后台建立起通信连接时（还未登录前），该方法被调用。",
          "type": "object"
        },
        "OnFrontDisconnected": {
          "title": "当客户端与交易后台通信连接断开时，该方法被调用。当发生这个情况后，API会自动重新连接，客户端可不做处理。",
          "type": "object",
          "properties": {
            "nReason": {
              "title": "错误原因       0x1001 网络读失败       0x1002 网络写失败       0x2001 接收心跳超时       0x2002 发送心跳失败       0x2003 收到错误报文",
              "type": "integer"
            }
          },
          "required": [
            "nReason"
          ]
        },
        "OnHeartBeatWarning": {
          "title": "心跳超时警告。当长时间未收到报文时，该方法被调用。",
          "type": "object",
          "properties": {
            "nTimeLapse": {
              "title": "距离上次接收报文的时间",
              "type": "integer"
            }
          },
          "required": [
            "nTimeLapse"
          ]
        },
        "OnRspUserLogin": {
          "title": "登录请求响应",
          "type": "object",
          "properties": {
            "pRspUserLogin": {
              "$ref": "structs.json#/definitions/CThostFtdcRspUserLoginField"
            },
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pRspUserLogin",
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        },
        "OnRspUserLogout": {
          "title": "登出请求响应",
          "type": "object",
          "properties": {
            "pUserLogout": {
              "$ref": "structs.json#/definitions/CThostFtdcUserLogoutField"
            },
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pUserLogout",
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        },
        "OnRspError": {
          "title": "错误应答",
          "type": "object",
          "properties": {
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        },
        "OnRspSubMarketData": {
          "title": "订阅行情应答",
          "type": "object",
          "properties": {
            "pSpecificInstrument": {
              "$ref": "structs.json#/definitions/CThostFtdcSpecificInstrumentField"
            },
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pSpecificInstrument",
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        },
        "OnRspUnSubMarketData": {
          "title": "取消订阅行情应答",
          "type": "object",
          "properties": {
            "pSpecificInstrument": {
              "$ref": "structs.json#/definitions/CThostFtdcSpecificInstrumentField"
            },
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pSpecificInstrument",
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        },
        "OnRspSubForQuoteRsp": {
          "title": "订阅询价应答",
          "type": "object",
          "properties": {
            "pSpecificInstrument": {
              "$ref": "structs.json#/definitions/CThostFtdcSpecificInstrumentField"
            },
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pSpecificInstrument",
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        },
        "OnRspUnSubForQuoteRsp": {
          "title": "取消订阅询价应答",
          "type": "object",
          "properties": {
            "pSpecificInstrument": {
              "$ref": "structs.json#/definitions/CThostFtdcSpecificInstrumentField"
            },
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pSpecificInstrument",
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        },
        "OnRtnDepthMarketData": {
          "title": "深度行情通知",
          "type": "object",
          "properties": {
            "pDepthMarketData": {
              "$ref": "structs.json#/definitions/CThostFtdcDepthMarketDataField"
            }
          },
          "required": [
            "pDepthMarketData"
          ]
        },
        "OnRtnForQuoteRsp": {
          "title": "询价通知",
          "type": "object",
          "properties": {
            "pForQuoteRsp": {
              "$ref": "structs.json#/definitions/CThostFtdcForQuoteRspField"
            }
          },
          "required": [
            "pForQuoteRsp"
          ]
        },
        "OnRspQryMulticastInstrument": {
          "title": "请求查询组播合约响应",
          "type": "object",
          "properties": {
            "pMulticastInstrument": {
              "$ref": "structs.json#/definitions/CThostFtdcMulticastInstrumentField"
            },
            "pRspInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcRspInfoField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            },
            "bIsLast": {
              "title": "是否最后一次返回",
              "type": "boolean"
            }
          },
          "required": [
            "pMulticastInstrument",
            "pRspInfo",
            "nRequestID",
            "bIsLast"
          ]
        }
      }
    },
    "CThostFtdcMdApi": {
      "title": "行情请求类",
      "properties": {
        "CreateFtdcMdApi": {
          "title": "创建MdApi",
          "type": "object",
          "properties": {
            "pszFlowPath": {
              "title": "存贮订阅信息文件的目录，默认为当前目录",
              "type": "string"
            },
            "bIsUsingUdp": {
              "type": "boolean"
            },
            "bIsMulticast": {
              "type": "boolean"
            }
          },
          "required": [
            "pszFlowPath",
            "bIsUsingUdp",
            "bIsMulticast"
          ]
        },
        "GetApiVersion": {
          "title": "获取API的版本信息",
          "type": "object"
        },
        "Release": {
          "title": "删除接口对象本身 不再使用本接口对象时,调用该函数删除接口对象",
          "type": "object"
        },
        "Init": {
          "title": "初始化 初始化运行环境,只有调用后,接口才开始工作",
          "type": "object"
        },
        "Join": {
          "title": "等待接口线程结束运行",
          "type": "object"
        },
        "GetTradingDay": {
          "title": "获取当前交易日 只有登录成功后,才能得到正确的交易日",
          "type": "object"
        },
        "RegisterFront": {
          "title": "注册前置机网络地址 网络地址的格式为：“protocol://ipaddress:port”，如：”tcp://127.0.0.1:17001”。  “tcp”代表传输协议，“127.0.0.1”代表服务器地址。”17001”代表服务器端口号。",
          "type": "object",
          "properties": {
            "pszFrontAddress": {
              "title": "前置机网络地址。",
              "type": "string"
            }
          },
          "required": [
            "pszFrontAddress"
          ]
        },
        "RegisterNameServer": {
          "title": "注册名字服务器网络地址 网络地址的格式为：“protocol://ipaddress:port”，如：”tcp://127.0.0.1:12001”。  “tcp”代表传输协议，“127.0.0.1”代表服务器地址。”12001”代表服务器端口号。 RegisterNameServer优先于RegisterFront",
          "type": "object",
          "properties": {
            "pszNsAddress": {
              "title": "名字服务器网络地址。",
              "type": "string"
            }
          },
          "required": [
            "pszNsAddress"
          ]
        },
        "RegisterFensUserInfo": {
          "title": "注册名字服务器用户信息",
          "type": "object",
          "properties": {
            "pFensUserInfo": {
              "$ref": "structs.json#/definitions/CThostFtdcFensUserInfoField"
            }
          },
          "required": [
            "pFensUserInfo"
          ]
        },
        "RegisterSpi": {
          "title": "注册回调接口",
          "type": "object",
          "properties": {
            "pSpi": {
              "type": "object",
              "title": "回调类 CThostFtdcMdSpi 的实例"
            }
          },
          "required": [
            "pSpi"
          ]
        },
        "SubscribeMarketData": {
          "title": "订阅行情。 ",
          "type": "object",
          "properties": {
            "ppInstrumentID": {
              "title": "合约ID",
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "nCount": {
              "title": "要订阅/退订行情的合约个数",
              "type": "integer"
            }
          },
          "required": [
            "ppInstrumentID",
            "nCount"
          ]
        },
        "UnSubscribeMarketData": {
          "title": "退订行情。 ",
          "type": "object",
          "properties": {
            "ppInstrumentID": {
              "title": "合约ID",
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "nCount": {
              "title": "要订阅/退订行情的合约个数",
              "type": "integer"
            }
          },
          "required": [
            "ppInstrumentID",
            "nCount"
          ]
        },
        "SubscribeForQuoteRsp": {
          "title": "订阅询价。 ",
          "type": "object",
          "properties": {
            "ppInstrumentID": {
              "title": "合约ID",
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "nCount": {
              "title": "要订阅/退订行情的合约个数",
              "type": "integer"
            }
          },
          "required": [
            "ppInstrumentID",
            "nCount"
          ]
        },
        "UnSubscribeForQuoteRsp": {
          "title": "退订询价。 ",
          "type": "object",
          "properties": {
            "ppInstrumentID": {
              "title": "合约ID",
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "nCount": {
              "title": "要订阅/退订行情的合约个数",
              "type": "integer"
            }
          },
          "required": [
            "ppInstrumentID",
            "nCount"
          ]
        },
        "ReqUserLogin": {
          "title": "用户登录请求",
          "type": "object",
          "properties": {
            "pReqUserLoginField": {
              "$ref": "structs.json#/definitions/CThostFtdcReqUserLoginField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            }
          },
          "required": [
            "pReqUserLoginField",
            "nRequestID"
          ]
        },
        "ReqUserLogout": {
          "title": "登出请求",
          "type": "object",
          "properties": {
            "pUserLogout": {
              "$ref": "structs.json#/definitions/CThostFtdcUserLogoutField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            }
          },
          "required": [
            "pUserLogout",
            "nRequestID"
          ]
        },
        "ReqQryMulticastInstrument": {
          "title": "请求查询组播合约",
          "type": "object",
          "properties": {
            "pQryMulticastInstrument": {
              "$ref": "structs.json#/definitions/CThostFtdcQryMulticastInstrumentField"
            },
            "nRequestID": {
              "title": "请求ID",
              "type": "integer"
            }
          },
          "required": [
            "pQryMulticastInstrument",
            "nRequestID"
          ]
        }
      }
    }
  }
}